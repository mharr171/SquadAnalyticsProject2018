<p id="notice"><%= notice %></p>

<h1>Feedback</h1>

<table class="table">
  <thead>
    <tr>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedback.each do |feedback| %>
      <tr>
        <td><!-- < %= feedback.data %> --></td>
        <td><!-- < %= link_to 'Show', feedback %> --></td>
